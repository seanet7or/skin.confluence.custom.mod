	<include name="HomeAddonsCommonLayout">
		<include>Window_OpenClose_Animation</include>
		<animation effect="slide" start="0,0" end="0,-195" time="0" condition="!Skin.HasSetting(RaiseMainMenuBar)">Conditional</animation>
		<animation type="Visible">
			<effect type="slide" start="0,30" end="0,0" delay="200" easing="out" tween="quadratic" time="200" />
			<effect type="fade" start="0" end="100" delay="200" time="200" />
		</animation>
		<animation type="Hidden">
			<effect type="zoom" start="100" end="70" center="auto" easing="in" tween="quadratic" time="300" />
			<effect type="fade" start="100" end="0" time="300" />
		</animation>
		<hitrect x="0" y="530" w="1280" h="120" />
		<posx>0</posx>
		<posy>190r</posy>
		<width>1280</width>
		<height>200</height>
		<pagecontrol></pagecontrol>
		<scrolltime>300</scrolltime>
		<focusposition>3</focusposition>
		<movement>2</movement>
		<orientation>Horizontal</orientation>
		<itemlayout height="200" width="182">
			<control type="image">
				<width>180</width>
				<height>120</height>
				<aspectratio aligny="bottom">keep</aspectratio>
				<bordertexture>buttons/nf_light.png</bordertexture>
				<bordersize>5</bordersize>
				<texture background="true">$INFO[ListItem.Icon]</texture>
			</control>
			<control type="label">
				<posx>91</posx>
				<posy>125</posy>
				<width>180</width>
				<height>20</height>
				<align>center</align>
				<label>$VAR[MainItemLabel]</label>
				<visible>!Skin.HasSetting(HideHomeAddonName)</visible>
			</control>
		</itemlayout>
		<focusedlayout height="200" width="182">
			<control type="image">
				<width>180</width>
				<height>120</height>
				<aspectratio aligny="bottom">keep</aspectratio>
				<bordertexture>buttons/nf_light.png</bordertexture>
				<bordersize>5</bordersize>
				<texture background="true">$INFO[ListItem.Icon]</texture>
				<visible>!ControlGroup(9002).HasFocus</visible>
			</control>
			<control type="image">
				<width>180</width>
				<height>120</height>
				<aspectratio aligny="bottom">keep</aspectratio>
				<bordertexture border="6">buttons/folder-focus_light.png</bordertexture>
				<bordersize>5</bordersize>
				<texture background="true">$INFO[ListItem.Icon]</texture>
				<visible>ControlGroup(9002).HasFocus</visible>
			</control>
			<control type="label">
				<posx>91</posx>
				<posy>125</posy>
				<width>180</width>
				<height>20</height>
				<textcolor>textfocus</textcolor>
				<align>center</align>
				<label>$VAR[MainItemLabel]</label>
				<visible>Control.HasFocus(9002)</visible>
				<visible>!Skin.HasSetting(HideHomeAddonName)</visible>
			</control>
			<control type="label">
				<posx>91</posx>
				<posy>125</posy>
				<width>180</width>
				<height>20</height>
				<align>center</align>
				<label>$VAR[MainItemLabel]</label>
				<visible>!Control.HasFocus(9002)</visible>
				<visible>!Skin.HasSetting(HideHomeAddonName)</visible>
			</control>
		</focusedlayout>
	</include>
